include includes/head.pug

body

  #site
    include includes/header.pug

    .intro
      .container.intro__container
        p.intro__appeal Volutpat porttitor mauris, sagittis mauris dictum nec pharetra feugiat ac. Faucibus platea leo egestas nulla sem nunc. Nisi, arcu porta aliquet blandit orci. Eu vel sodales et enim.
        .intro__body
          button(type="button").intro__body-close
            svg.icon
              use(xlink:href="./images/sprite.svg#close")
          .intro__body-content
            h1.intro__body-content-title At vel malesuada sed proin. Eu faucibus purus ullamcorper nibh consequat neque in iaculis
            a.md-none.btn.btn--size-lg.btn--theme-dark.intro__body-content-btn Подробнее
          picture.intro__body-picture
            source(srcset="./images/intro-hero-1.webp", type="image/webp")
            source(srcset="./images/intro-hero-1@2x.webp 2x", type="image/webp")
            img(src="./images/intro-hero-1.jpg", srcset="./images/intro-hero-1@2x.jpg 2x", alt="At vel malesuada sed proin. Eu faucibus purus ullamcorper nibh consequat neque in iaculis")
          a.md-visible.btn.btn--size-lg.btn--theme-dark.intro__body-content-btn Подробнее

    main(role="main").main
      section.feed-section
        .container.feed-section__container
          .feed-section__header
            a(href="#").btn.btn--size-sm.btn--theme-gray.feed-section__header-btn-profit Профит
            .group-btn-text.feed-section__header-filter
              .group-btn-text__inner
                .group-btn-text__item
                  p.md-none.group-btn-text__item-title Количество на одной странице
                  p.md-visible.group-btn-text__item-title Кол-во на стр.
                  select(name="filter").js-select-init.group-btn-text__item-el
                    option(value="10") 10
                    option(value="20") 20
                    option(value="30") 30
          .feed-section__list
            article.event-card.feed-section__list-card
              table.event-card__table
