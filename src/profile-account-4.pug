body

  #site
    include includes/header.pug

    main(role="main").main
      section.profile-layout
        .container.profile-layout__container
          aside.profile-aside.profile-layout__aside
            .profile-aside__header
              h1.profile-aside__header-title Профиль
              a(href="#").profile-aside__header-login логин
              button(type="button").md-visible.js-toggle-aside-nav.profile-aside__header-btn-toggle
                svg.icon
                  use(xlink:href="./images/sprite.svg#toggle-aside-nav")
            nav.profile-aside__nav
              ul.profile-aside__nav-list
                li.profile-aside__nav-item
                  a(href="profile-account-1.html").profile-aside__nav-link Аккаунт
                li.profile-aside__nav-item
                  a(href="profile-account-2.html").profile-aside__nav-link Настройки
                li.profile-aside__nav-item
                  a(href="profile-account-3.html").profile-aside__nav-link Скрытые вилки
                li.profile-aside__nav-item.is-active
                  a(href="profile-account-4.html").profile-aside__nav-link Букмекеры
                li.profile-aside__nav-item
                  a(href="profile-account-5.html").profile-aside__nav-link История ставок
                li.profile-aside__nav-item
                  a(href="profile-account-6.html").profile-aside__nav-link Платежи
                li.profile-aside__nav-item
                  a(href="profile-account-7.html").profile-aside__nav-link Партнерская программа
          .profile-main.profile-layout__main
            .profile-main__account-list
              .profile-settings-card.profile-main__account-list-card
                .profile-settings-card__b-header
                  .profile-settings-card__b-header-title Зеркала
                  a(href="#").profile-settings-card__b-header-settings Настройки
                  form(action="#").search-form.profile-settings-card__b-header-search
                    input(type="search", name="q", required, placeholder="Найти букмекера").input-el.search-form__input
                    button(type="submit").search-form__btn
                      svg.icon
                        use(xlink:href="./images/sprite.svg#search")
                .bookmaker-table.profile-settings-card__b-block
                  .bookmaker-table__row.bookmaker-table__row--header
                    .bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__title Букмекер
                    .bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__title Домен
                    .bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__title Возможности
                    .bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__title Язык платформы
                  .bookmaker-table__row.bookmaker-table__row--body
                    div(data-table-title-mobile="Букмекер").bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__name 1xbet
                    div(data-table-title-mobile="Домен").bookmaker-table__col.bookmaker-table__col--size-3
                      label.checkbox-label.bookmaker-table__domain
                        input(type="checkbox", name="domain", value="Ввести свой домен").checkbox-label__control
                        span.checkbox-label__inner
                          span.checkbox-label__inner-icon
                          span.checkbox-label__inner-text Ввести свой домен
                    div(data-table-title-mobile="Возможности").bookmaker-table__col.bookmaker-table__col--size-3
                      .bookmaker-table__capabilities-list
                        .capabilities-item.capabilities-item--type-1.bookmaker-table__capabilities-list-item
                        .capabilities-item.capabilities-item--type-2.bookmaker-table__capabilities-list-item
                        .capabilities-item.capabilities-item--type-3.bookmaker-table__capabilities-list-item
                    div(data-table-title-mobile="Язык платформы").bookmaker-table__col.bookmaker-table__col--size-3
